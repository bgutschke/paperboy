{
  "name": "@neoskop/paperboy",
  "version": "1.1.1",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Philipp Siekmann <siekmann@neoskop.de>",
  "repository": "https://github.com/neoskop/paperboy",
  "homepage": "https://neolabs.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "lint": "tslint -p tslint.json",
    "start": "npm run build && nodemon --inspect -e ts,json --watch 'src' --exec ts-node src/index.ts",
    "test": "nyc mocha --require ts-node/register --require source-map-support/register --recursive src/**/*.spec.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/express": "4.16.1",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.19",
    "@types/shelljs": "0.8.2",
    "@types/winston": "2.4.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "concurrently": "4.1.0",
    "mocha": "5.2.0",
    "mocha-mongoose": "1.2.0",
    "nodemon": "1.18.9",
    "nyc": "13.1.0",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.10",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.2.4"
  },
  "dependencies": {
    "shelljs": "0.8.3",
    "winston": "3.2.1"
  }
}
